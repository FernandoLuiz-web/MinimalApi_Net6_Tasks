  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:48218",
      "sslPort": 44372
    }
  },
  /*
    Mudar a opção launchBrowser para true para obter retorno web

    removido: launchUrl: "swagger" pois swagger não está sendo usado no projeto
    */
